{
    "name": "ems/framework",
    "description": "A framework agnostic php library which provides the basics for cmsable and other frameworks",
    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "illuminate/mail": "5.*"
    },
    "replace": {
        "ems/contracts": "self.version",
        "ems/core": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Ems\\": "src/Ems"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Tils",
            "email": "mtils@web-utils.de"
        }
    ],
    "minimum-stability": "dev"
}